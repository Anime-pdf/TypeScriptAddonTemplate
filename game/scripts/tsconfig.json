{
    "compilerOptions": {
        "rootDir": "vscripts",
        "target": "esnext",
        "lib": ["esnext"],
        "types": ["lua-types/jit"],
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "strict": true
    },
    "tstl": {
        "luaTarget": "JIT",
        "sourceMapTraceback": true
    }
}
